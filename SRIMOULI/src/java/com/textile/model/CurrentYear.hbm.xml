<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.textile.model.CurrentYear" table="current_year" catalog="srimouli2" optimistic-lock="version">
       <id name="ids" type="java.lang.Integer">
            <column name="ids" />
            <generator class="identity" />
        </id>
        <property name="dateFrom" type="string">
            <column name="date_from" length="20" />
        </property>
        <property name="dateTo" type="string">
            <column name="date_to" length="20" />
        </property>
       <property name="status" type="string">
            <column name="status" length="20" />
        </property>
       
         </class>
</hibernate-mapping>
